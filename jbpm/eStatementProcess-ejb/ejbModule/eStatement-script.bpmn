  <process processType="Private" isExecutable="true" id="eStatement-scriptId" name="eStatement-script" tns:packageName="bg.bulsi.estatement.process" >

    <extensionElements>
     <tns:import name="org.jboss.seam.core.Expressions" />
     <tns:import name="org.jboss.seam.core.Expressions.ValueExpression" />
     <tns:import name="bg.bulsi.estatement.validation.impl.DummyValidation" />
