     <tns:import name="bg.bulsi.estatement.epayments.impl.DummyPaymentMediator" />
    </extensionElements>
    <!-- process variables -->
    <property id="documentId" itemSubjectRef="_documentIdItem"/>
    <property id="paymentCode" itemSubjectRef="_paymentCodeItem"/>
    <property id="validationResult" itemSubjectRef="_validationResultItem"/>

    <!-- nodes -->
    <scriptTask id="_17" name="StoreXML" scriptFormat="http://www.java.com/java" >
      <script>System.out.println("StoreXML node start.");

Expressions expressions = Expressions.instance();
ValueExpression&lt;String&gt; exprDoc = expressions.createValueExpression("#{submittedDocument}", String.class);
String submittedDocument = exprDoc.getValue();

ValueExpression&lt;DummyRegPortal&gt; exprRegPortal = expressions.createValueExpression("#{DummyRegPortal}", DummyRegPortal.class);
DummyRegPortal regPortal = exprRegPortal.getValue();

documentId = regPortal.registerXmlDocument(submittedDocument);
kcontext.setVariable("documentId", documentId);

System.out.println("StoreXML node end.");
</script>
    </scriptTask>
    <startEvent id="_1" name="Start" />
    <endEvent id="_2" name="End" >
        <terminateEventDefinition/>
    </endEvent>
    <scriptTask id="_19" name="GetPayments" scriptFormat="http://www.java.com/java" >
      <script>System.out.println("GetPayments node start.");

String docId = (String)kcontext.getVariable("documentId");

Expressions expressions = Expressions.instance();
ValueExpression&lt;DummyRegIo&gt; exprIo = expressions.createValueExpression("#{DummyRegIo}", DummyRegIo.class);
DummyRegIo io = exprIo.getValue();

paymentCode = (Integer)io.getPaymentCode((String)docId);

kcontext.setVariable("paymentCode", paymentCode);

System.out.println("GetPayments node end.");
</script>
    </scriptTask>
    <scriptTask id="_18" name="Validate" scriptFormat="http://www.java.com/java" >
      <script>System.out.println("Validate node start.");

Expressions expressions = Expressions.instance();
ValueExpression&lt;DummyRegPortal&gt; exprRegPortal = expressions.createValueExpression("#{DummyRegPortal}", DummyRegPortal.class);
DummyRegPortal regPortal = exprRegPortal.getValue();

String document = (String)regPortal.getXmlDocument(documentId);

ValueExpression&lt;DummyValidation&gt; exprValidation = expressions.createValueExpression("#{DummyValidation}", DummyValidation.class);
DummyValidation validation = exprValidation.getValue();

String result = (String)validation.validate(document);
kcontext.setVariable("validationResult", result);

System.out.println("Validate node end.");
</script>
    </scriptTask>
    <scriptTask id="_21" name="RegisterInESB" >
      <script>System.out.println("Register in ESB node.");
</script>
    </scriptTask>
    <scriptTask id="_20" name="PayTaxes" scriptFormat="http://www.java.com/java" >
      <script>System.out.println("PayTaxes node start.");

String docId = (String)kcontext.getVariable("documentId");

paymentCode = (Integer)kcontext.getVariable("paymentCode");

Expressions expressions = Expressions.instance();
ValueExpression&lt;DummyPaymentMediator&gt; exprPaymentMediator = expressions.createValueExpression("#{DummyPaymentMediator}", DummyPaymentMediator.class);
DummyPaymentMediator paymentMediator = exprPaymentMediator.getValue();


paymentMediator.createPayment((String)docId, (Integer)paymentCode);

System.out.println("PayTaxes node end.");</script>
    </scriptTask>
    <inclusiveGateway id="_6" name="PaidService" gatewayDirection="Diverging" />
    <scriptTask id="_22" name="GenerateRAM" >
      <script>System.out.println("GenerateEAM node.");</script>
    </scriptTask>
    <parallelGateway id="_8" name="Gateway" gatewayDirection="Converging" />

    <!-- connections -->
    <sequenceFlow id="_1-_17" sourceRef="_1" targetRef="_17" />
    <sequenceFlow id="_22-_2" sourceRef="_22" targetRef="_2" />
    <sequenceFlow id="_18-_19" sourceRef="_18" targetRef="_19" />
    <sequenceFlow id="_17-_18" sourceRef="_17" targetRef="_18" />
    <sequenceFlow id="_8-_21" sourceRef="_8" targetRef="_21" />
    <sequenceFlow id="_6-_20" sourceRef="_6" targetRef="_20" >
      <conditionExpression xsi:type="tFormalExpression" language="http://www.java.com/java" >paymentCode = (Integer)kcontext.getVariable("paymentCode");
// Code 0 means no payments
if(paymentCode != 0){
	return true;
}else {
	return false;
}
	</conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="_19-_6" sourceRef="_19" targetRef="_6" />
    <sequenceFlow id="_21-_22" sourceRef="_21" targetRef="_22" />
    <sequenceFlow id="_6-_8" sourceRef="_6" targetRef="_8" >
      <conditionExpression xsi:type="tFormalExpression" language="http://www.java.com/java" >paymentCode = (Integer)kcontext.getVariable("paymentCode");
// Code 0 means no payments
if(paymentCode == 0){
	return true;
}else {
	return false;
}
	</conditionExpression>
    </sequenceFlow>
    <sequenceFlow id="_20-_8" sourceRef="_20" targetRef="_8" />

  </process>

